    "@midwayjs/koa": "^2.3.0",
    "@midwayjs/logger": "^2.11.0",
    "@types/cache-manager": "^3.4.0",
    "cache-manager": "^3.4.3",
    "glob": "^7.1.7",
    "jsonwebtoken": "^8.5.1",
    "koa-bodyparser": "^4.3.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
