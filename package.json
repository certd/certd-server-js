    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@midwayjs/orm": "^1.3.1",
    "@types/jest": "^26.0.10",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/node": "14",
